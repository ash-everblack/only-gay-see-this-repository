Show_Assets = false 

getgenv().AshDestroyed = false
----------------------------
local cloneref = cloneref or function(o) return o end

Workspace = cloneref(game:GetService("Workspace"))
Players = cloneref(game:GetService("Players"))
Lighting = cloneref(game:GetService("Lighting"))
RunService = cloneref(game:GetService("RunService"))
UserInputService = cloneref(game:GetService("UserInputService"))
TweenService = cloneref(game:GetService("TweenService"))
TextService = cloneref(game:GetService("TextService"))
GuiService = cloneref(game:GetService("GuiService"))
HttpService = cloneref(game:GetService("HttpService"))
coreGui = gethui() or cloneref(game:GetService("CoreGui"))

LocalPlayer = Players.LocalPlayer
Camera = Workspace.CurrentCamera
Mouse = LocalPlayer:GetMouse()

uiFile = string.char(65, 115, 104, 98, 111, 114, 110, 110, 72, 117, 98, 47, 65, 117, 116, 111, 67, 108, 111, 115, 101, 85, 73, 46, 106, 115, 111, 110)
posFile = string.char(65, 115, 104, 98, 111, 114, 110, 110, 72, 117, 98, 47, 85, 73, 76, 105, 98, 98, 116, 110, 80, 111, 115, 46, 106, 115, 111, 110)
local MIN_TOP_MARGIN = 8
local MIN_BOTTOM_MARGIN = 8

local function loadPosition()
    if isfile and isfile(posFile) then
        local ok, data = pcall(function()
            return HttpService:JSONDecode(readfile(posFile))
        end)
        if ok and data.x and data.y then
            return data.x, data.y
        end
    end
    return nil, nil
end

local function savePosition(x, y)
    if writefile then
        pcall(function()
            writefile(posFile, HttpService:JSONEncode({x = x, y = y}))
        end)
    end
end

local function clampToViewport(button, x, y)
    while button.AbsoluteSize.X == 0 or button.AbsoluteSize.Y == 0 do
        RunService.RenderStepped:Wait()
    end
    while Camera.ViewportSize.X == 0 or Camera.ViewportSize.Y == 0 do
        RunService.RenderStepped:Wait()
    end
    local absSize = button.AbsoluteSize
    local viewport = Camera.ViewportSize
    local minX, minY = 0, MIN_TOP_MARGIN
    local maxX = math.max(minX, viewport.X - absSize.X)
    local maxY = math.max(minY, viewport.Y - absSize.Y - MIN_BOTTOM_MARGIN)
    x = math.clamp(x, minX, maxX)
    y = math.clamp(y, minY, maxY)
    button.Position = UDim2.new(0, x, 0, y)
    savePosition(x, y)
    return x, y
end

local Mobile
if RunService:IsStudio() then
	Mobile = false
else
	Mobile = table.find({Enum.Platform.IOS, Enum.Platform.Android}, UserInputService:GetPlatform()) ~= nil
end

