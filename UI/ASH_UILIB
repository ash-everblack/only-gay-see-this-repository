er.Name.."\046\106\115\111\110")local r=h.Folder..("\047\115\101\116\116\105\110\103\115\047"..N)local L=N:match("\040\046\043\041\037\046\106\115\111\110\036")or N local l={configName=L;objects={}}for t,N in pairs(h.Options)do if h.Parser[N.Type]and not h.Ignore[t]then table.insert(l.objects,h.Parser[N.Type].Save(t,N))end end local q,Y=pcall(function()return HttpService:JSONEncode(l)end)if not q then return false,"\070\097\105\108\101\100 \116\111 \101\110\099\111\100\101 \100\097\116\097"end writefile(r,Y)return true end function NQ.Load(h,t)if not LocalPlayer then return false,"\076\111\099\097\108\080\108\097\121\101\114 \110\111\116 \102\111\117\110\100"end if not h.Library then return false,"\076\105\098\114\097\114\121 \110\111\116 \115\101\116"end local N=t or"\083\101\116\116\105\110\103\115\095"..(LocalPlayer.Name.."\046\106\115\111\110")local r=h.Folder..("\047\115\101\116\116\105\110\103\115\047"..N)if not isfile(r)then return false,"\073\110\118\097\108\105\100 \102\105\108\101"end local L=readfile(r)local l,q=pcall(function()return HttpService:JSONDecode(L)end)if not l then delfile(r)return false,"\068\101\099\111\100\101 \101\114\114\111\114 \040\102\105\108\101 \100\101\108\101\116\101\100\041"end local Y=tick()h.Library:Notify({Title=(getgenv()).AshTitle or"\067\111\110\102\105\103 \076\111\097\100\101\114",Content="\076\111\097\100\105\110\103 \099\111\110\102\105\103\046\046\046",SubContent="\080\108\101\097\115\101 \119\097\105\116 \119\104\105\108\101 \115\101\116\116\105\110\103\115 \097\114\101 \098\101\105\110\103 \097\112\112\108\105\101\100\046",Duration=5})local d,z={},{}for t,N in ipairs(q.objects)do local r=h.Parser[N.type]if r and h.Options[N.idx]then local t=function()r.Load(N.idx,N)end if N.type=="\084\111\103\103\108\101"then table.insert(z,t)else table.insert(d,t)end end end task.spawn(function()for t,h in ipairs(d)do h()task.wait(.01)end for t,h in ipairs(z)do h()task.wait(.01)end local t=math.abs(Y-tick())local N=string.format("\037\046\050\102",t)local r if t>10 then r="\083\117\099\099\101\115\115\102\117\108\108\121 \108\111\097\100\101\100 \105\110 "..(N.."\115\046 \084\104\097\110\107 \121\111\117 \102\111\114 \121\111\117\114 \112\097\116\105\101\110\099\101 \240\159\165\176")elseif t>5 then r="\076\097\115\116 \067\111\110\102\105\103\117\114\097\116\105\111\110 \104\097\115 \098\101\101\110 \108\111\097\100\101\100 \113\117\105\099\107\108\121 \119\105\116\104\105\110 "..(N.."\115\033")else r="\067\111\110\102\105\103\117\114\097\116\105\111\110 \104\097\115 \098\101\101\110 \108\111\097\100\101\100 \119\105\116\104\105\110 "..(N.."\115")end h.Library:Notify({Title=(getgenv()).AshTitle or"\067\111\110\102\105\103 \076\111\097\100\101\114",Content="\067\111\110\102\105\103 \076\111\097\100\101\100\033";SubContent=r,Duration=6})end)return true end function NQ.IgnoreThemeSettings(t)t:SetIgnoreIndexes({"\073\110\116\101\114\102\097\099\101\084\104\101\109\101";"\065\099\114\121\108\105\099\084\111\103\103\108\101","\084\114\097\110\115\112\097\114\101\110\116\084\111\103\103\108\101","\077\101\110\117\075\101\121\098\105\110\100"})end function NQ.BuildFolderTree(t)if not isfolder(t.Folder)then makefolder(t.Folder)end local h=t.Folder.."\047\115\101\116\116\105\110\103\115"if not isfolder(h)then makefolder(h)end end function NQ.RefreshConfigList(t)local h={}for t,N in ipairs(listfiles(t.Folder.."\047\115\101\116\116\105\110\103\115"))do if N:sub(-5)=="\046\106\115\111\110"then local t=N:match("\040\091\094\047\092\093\043\041\037\046\106\115\111\110\036")if t and t~="\111\112\116\105\111\110\115"then table.insert(h,t)end end end return h end function NQ.GetAutoloadConfig(t)local h=t.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(h)then return readfile(h)end return nil end function NQ.ClearAutoload(t)local h=t.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(h)then delfile(h)end end function NQ.DeleteConfig(h,t)if not t or t==""then return false,"\073\110\118\097\108\105\100 \099\111\110\102\105\103 \110\097\109\101"end if not t:match("\037\046\106\115\111\110\036")then t=t.."\046\106\115\111\110"end local N=h.Folder..("\047\115\101\116\116\105\110\103\115\047"..t)if not isfile(N)then return false,"\067\111\110\102\105\103 \102\105\108\101 \110\111\116 \102\111\117\110\100"end local r=h.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(r)and readfile(r)==t then delfile(r)end delfile(N)return true end function NQ.LoadAutoloadConfig(t)local h=t.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(h)then local N=readfile(h)local r,L=t:Load(N)t.Library:Notify({Title="\073\110\116\101\114\102\097\099\101",Content="\067\111\110\102\105\103 \108\111\097\100\101\114",SubContent=r and"\065\117\116\111 \108\111\097\100\101\100 \099\111\110\102\105\103 "..N or"\070\097\105\108\101\100 \116\111 \108\111\097\100 \097\117\116\111\108\111\097\100 \099\111\110\102\105\103\058 "..L,Duration=7})end end function NQ.BuildConfigSection(h,t)assert(h.Library,"\077\117\115\116 \115\101\116 \083\097\118\101\077\097\110\097\103\101\114\046\076\105\098\114\097\114\121")local N=t:AddSection("\067\111\110\102\105\103\117\114\097\116\105\111\110")N:AddInput("\083\097\118\101\077\097\110\097\103\101\114\095\067\111\110\102\105\103\078\097\109\101",{Title="\067\111\110\102\105\103 \110\097\109\101"})N:AddDropdown("\083\097\118\101\077\097\110\097\103\101\114\095\067\111\110\102\105\103\076\105\115\116",{Title="\067\111\110\102\105\103 \108\105\115\116",Values=h:RefreshConfigList();AllowNull=true})local function r(t,N)h.Library:Notify({Title="\073\110\116\101\114\102\097\099\101";Content="\067\111\110\102\105\103 \108\111\097\100\101\114",SubContent=N;Duration=7})end N:AddButton({Title="\067\114\101\097\116\101 \099\111\110\102\105\103";Callback=function()local t=h.Options.SaveManager_ConfigName.Value if t:match("\094\037\115\042\036")then return r("\073\110\116\101\114\102\097\099\101","\073\110\118\097\108\105\100 \099\111\110\102\105\103 \110\097\109\101 \040\101\109\112\116\121\041")end local N,L=h:Save(t.."\046\106\115\111\110")r("\073\110\116\101\114\102\097\099\101",N and"\067\114\101\097\116\101\100 \099\111\110\102\105\103 "..t or"\070\097\105\108\101\100 \116\111 \115\097\118\101 \099\111\110\102\105\103\058 "..L)h.Options.SaveManager_ConfigList:SetValues(h:RefreshConfigList())h.Options.SaveManager_ConfigList:SetValue(t)end})N:AddButton({Title="\076\111\097\100 \099\111\110\102\105\103";Callback=function()local t=h.Options.SaveManager_ConfigList.Value if not t or t==""then return r("\073\110\116\101\114\102\097\099\101","\078\111 \099\111\110\102\105\103 \115\101\108\101\099\116\101\100")end if not t:match("\037\046\106\115\111\110\036")then t=t.."\046\106\115\111\110"end local N,L=h:Load(t)r("\073\110\116\101\114\102\097\099\101",N and"\076\111\097\100\101\100 \099\111\110\102\105\103 "..t or"\070\097\105\108\101\100 \116\111 \108\111\097\100 \099\111\110\102\105\103\058 "..L)end})N:AddButton({Title="\079\118\101\114\119\114\105\116\101 \099\111\110\102\105\103",Callback=function()local t=h.Options.SaveManager_ConfigList.Value if not t or t==""then return r("\073\110\116\101\114\102\097\099\101","\078\111 \099\111\110\102\105\103 \115\101\108\101\099\116\101\100")end if not t:match("\037\046\106\115\111\110\036")then t=t.."\046\106\115\111\110"end local N,L=h:Save(t)r("\073\110\116\101\114\102\097\099\101",N and"\079\118\101\114\119\114\111\116\101 \099\111\110\102\105\103 "..t or"\070\097\105\108\101\100 \116\111 \111\118\101\114\119\114\105\116\101 \099\111\110\102\105\103\058 "..L)end})N:AddButton({Title="\068\101\108\101\116\101 \099\111\110\102\105\103";Callback=function()local t=h.Options.SaveManager_ConfigList.Value if not t or t==""then return r("\073\110\116\101\114\102\097\099\101","\078\111 \0