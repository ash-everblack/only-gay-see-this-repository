)return setmetatable({_onStep=x.new(),_onStart=x.new(),_onComplete=x.new()},F)end function F.onStep(h,t)return h._onStep:connect(t)end function F.onStart(h,t)return h._onStart:connect(t)end function F.onComplete(h,t)return h._onComplete:connect(t)end function F.start(t)if not t._connection then t._connection=RunService.RenderStepped:Connect(function(h)t:step(h)end)end end function F.stop(t)if t._connection then t._connection:Disconnect()t._connection=nil end end F.destroy=F.stop F.step=D F.getValue=D F.setGoal=D function F.__tostring(t)return"\077\111\116\111\114"end local e=setmetatable({},F)e.__index=e function e.new(t,h)assert(t,"\077\105\115\115\105\110\103 \097\114\103\117\109\101\110\116 \035\049\058 \105\110\105\116\105\097\108\086\097\108\117\101")assert(typeof(t)=="\110\117\109\098\101\114","\105\110\105\116\105\097\108\086\097\108\117\101 \109\117\115\116 \098\101 \097 \110\117\109\098\101\114\033")local N=setmetatable(F.new(),e)if h~=nil then N._useImplicitConnections=h else N._useImplicitConnections=true end N._goal=nil N._state={complete=true;value=t}return N end function e.step(h,t)if h._state.complete then return true end local N=h._goal:step(h._state,t)h._state=N h._onStep:fire(N.value)if N.complete then if h._useImplicitConnections then h:stop()end h._onComplete:fire()end return N.complete end function e.getValue(t)return t._state.value end function e.setGoal(h,t)h._state.complete=false h._goal=t h._onStart:fire()if h._useImplicitConnections then h:start()end end function e.__tostring(t)return"\077\111\116\111\114\040\083\105\110\103\108\101\041"end local U=setmetatable({},F)U.__index=U local function b(t)if w(t)then return t end local h=typeof(t)if h=="\110\117\109\098\101\114"then return e.new(t,false)elseif h=="\116\097\098\108\101"then return U.new(t,false)end error(("\085\110\097\098\108\101 \116\111 \099\111\110\118\101\114\116 \037\113 \116\111 \109\111\116\111\114\059 \116\121\112\101 \037\115 \105\115 \117\110\115\117\112\112\111\114\116\10