)return setmetatable({_onStep=T.new(),_onStart=T.new();_onComplete=T.new()},w)end function w.onStep(W,F)return W._onStep:connect(F)end function w.onStart(W,F)return W._onStart:connect(F)end function w.onComplete(W,F)return W._onComplete:connect(F)end function w.start(F)if not F._connection then F._connection=RunService.RenderStepped:Connect(function(W)F:step(W)end)end end function w.stop(F)if F._connection then F._connection:Disconnect()F._connection=nil end end w.destroy=w.stop w.step=U w.getValue=U w.setGoal=U function w.__tostring(F)return"\077\111\116\111\114"end local m=setmetatable({},w)m.__index=m function m.new(F,W)assert(F,"\077\105\115\115\105\110\103 \097\114\103\117\109\101\110\116 \035\049\058 \105\110\105\116\105\097\108\086\097\108\117\101")assert(typeof(F)=="\110\117\109\098\101\114","\105\110\105\116\105\097\108\086\097\108\117\101 \109\117\115\116 \098\101 \097 \110\117\109\098\101\114\033")local P=setmetatable(w.new(),m)if W~=nil then P._useImplicitConnections=W else P._useImplicitConnections=true end P._goal=nil P._state={complete=true;value=F}return P end function m.step(W,F)if W._state.complete then return true end local P=W._goal:step(W._state,F)W._state=P W._onStep:fire(P.value)if P.complete then if W._useImplicitConnections then W:stop()end W._onComplete:fire()end return P.complete end function m.getValue(F)return F._state.value end function m.setGoal(W,F)W._state.complete=false W._goal=F W._onStart:fire()if W._useImplicitConnections then W:start()end end function m.__tostring(F)return"\077\111\116\111\114\040\083\105\110\103\108\101\041"end local o=setmetatable({},w)o.__index=o local function A(F)if s(F)then return F end local W=typeof(F)if W=="\110\117\109\098\101\114"then return m.new(F,false)elseif W=="\116\097\098\108\101"then return o.new(F,false)end error(("\085\110\097\098\108\101 \116\111 \099\111\110\118\101\114\116 \037\113 \116\111 \109\111\116\111\114\059 \116\121\112\101 \037\115 \105\115 \117\110\115\117\112\112\111\114\116\10