17\116\111\108\111\097\100 \099\111\110\102\105\103\058 \110\111\110\101")r("\073\110\116\101\114\102\097\099\101","\068\105\115\097\098\108\101\100 \097\117\116\111\108\111\097\100 \102\111\114 "..t)else writefile(h.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116",t)AutoloadButton:SetDesc("\067\117\114\114\101\110\116 \097\117\116\111\108\111\097\100 \099\111\110\102\105\103\058 "..t)r("\073\110\116\101\114\102\097\099\101","\083\101\116 "..(t.." \116\111 \097\117\116\111 \108\111\097\100"))end end})local l=h.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(l)then L:SetDesc("\067\117\114\114\101\110\116 \097\117\116\111\108\111\097\100 \099\111\110\102\105\103\058 "..readfile(l))end N:AddInput("\083\097\118\101\077\097\110\097\103\101\114\095\074\083\079\078\073\110\112\117\116",{Title="\073\109\112\111\114\116 \102\114\111\109 \074\083\079\078",Default="";Placeholder="\080\097\115\116\101 \121\111\117\114 \099\111\110\102\105\103 \074\083\079\078 \104\101\114\101",Numeric=false,Finished=true,Callback=function(t)if not t or t:match("\094\037\115\042\036")then return end local N,L=pcall(function()return HttpService:JSONDecode(t)end)if not N then r("\073\110\118\097\108\105\100 \074\083\079\078","\070\097\105\108\101\100 \116\111 \112\097\114\115\101 \074\083\079\078 \102\111\114\109\097\116\046 \080\108\101\097\115\101 \099\104\101\099\107 \121\111\117\114 \099\111\110\102\105\103\117\114\097\116\105\111\110\046")return end if not L.objects or type(L.objects)~="\116\097\098\108\101"then r("\073\110\118\097\108\105\100 \067\111\110\102\105\103","\074\083\079\078 \115\116\114\117\099\116\117\114\101 \105\115 \105\110\118\097\108\105\100\046 \077\105\115\115\105\110\103 \'\111\098\106\101\099\116\115\' \097\114\114\097\121\046")return end local l=L.configName or"\073\109\112\111\114\116\101\100\067\111\110\102\105\103\095"..os.time()if not l:match("\037\046\106\115\111\110\036")then l=l.."\046\106\115\111\110"end local q,Y={},{}local d=0 for t,N in ipairs(L.objects)do local r=h.Parser[N.type]if r and h.Options[N.idx]then local t=function()r.Load(N.idx,N)end if N.type=="\084\111\103\103\108\101"then table.insert(Y,t)else table.insert(q,t)end d=d+1 end end task.spawn(function()for t,h in ipairs(q)do h()task.wait(.01)end for t,h in ipairs(Y)do h()task.wait(.01)end local t=h.Folder..("\047\115\101\116\116\105\110\103\115\047"..l)local N,z=pcall(function()return HttpService:JSONEncode(L)end)if N then writefile(t,z)r("\067\111\110\102\105\103 \073\109\112\111\114\116\101\100","\083\117\099\099\101\115\115\102\117\108\108\121 \108\111\097\100\101\100 \097\110\100 \115\097\118\101\100 "..(d..(" \115\101\116\116\105\110\103\115 \097\115 \'"..(l:sub(1,-6).."\'\046"))))h.Options.SaveManager_ConfigList:SetValues(h:RefreshConfigList())h.Options.SaveManager_ConfigList:SetValue(l:sub(1,-6))else r("\083\097\118\101 \069\114\114\111\114","\067\111\110\102\105\103 \108\111\097\100\101\100 \098\117\116 \102\097\105\108\101\100 \116\111 \115\097\118\101 \116\111 \102\105\108\101\046")end end)h.Options.SaveManager_JSONInput:SetValue("")end})local q,Y=pcall(function()local t=h.Folder..("\047\115\101\116\116\105\110\103\115\047\083\101\116\116\105\110\103\115\095"..(LocalPlayer.Name.."\046\106\115\111\110"))local r=h.Folder..("\047"..(LocalPlayer.Name.."\095\073\115\065\117\116\111\076\111\097\100\046\116\120\116"))local L=false if isfile(r)then L=readfile(r)=="\116\114\117\101"end;(getgenv()).isAutoSaving=L local l=N:AddToggle("\065\117\116\111\083\097\118\101\067\111\110\102\105\103",{Title="\065\117\116\111 \083\097\118\101 \067\111\110\102\105\103";Description="\065\117\116\111 \076\111\097\100 \097\110\100 \083\097\118\101 \099\117\114\114\101\110\116 \099\111\110\102\105\103\046";Default=false})l:SetValue(L)local q l:OnChanged(function(N)(getgenv()).isAutoSaving=N if writefile then writefile(r,tostring(N))end if(getgenv()).isAutoSaving then if L and isfile(t)then local t,N=h:Load("\083\101\116\116\105\110\103\115\095"..(LocalPlayer.Name.."\046\106\115\111\110"))if t then if h.Options.ToggleRefreshedNpc then h.Options.ToggleRefreshedNpc:SetValue(true)end else warn("\070\097\105\108\101\100 \116\111 \108\111\097\100 \115\101\116\116\105\110\103\115\058",N)end end if q then task.cancel(q)q=nil end q=task.spawn(function()while(getgenv()).isAutoSaving and not(getgenv()).AshDestroyed do if h.Options then task.spawn(function()local t,N=h:Save("\083\101\116\116\105\110\103\115\095"..(LocalPlayer.Name.."\046\106\115\111\110"))if not t then warn("\070\097\105\108\101\100 \116\111 \115\097\118\101 \115\101\116\116\105\110\103\115\058",N)end end)end task.wait(.5)end end)elseif q then task.cancel(q)q=nil end end)end)if not q then warn("\069\114\114\111\114 \105\110 \097\117\116\111-\115\097\118\101\058",Y)end h:SetIgnoreIndexes({"\083\097\118\101\077\097\110\097\103\101\114\095\067\111\110\102\105\103\076\105\115\116";"\083\097\118\101\077\097\110\097\103\101\114\095\067\111\110\102\105\103\078\097\109\101";"\065\117\116\111\083\097\118\101\067\111\110\102\105\103","\083\097\118\101\077\097\110\097\103\101\114\095\074\083\079\078\073\110\112\117\116"})end NQ:BuildFolderTree()end local rQ={}do rQ.Folder="\070\108\117\101\110\116\083\101\116\116\105\110\103\115"rQ.Settings={Acrylic=true,Transparency=true,MenuKeybind="\077"}function rQ.SetTheme(h,t)rQ.Settings.Theme=t end function rQ.SetFolder(h,t)h.Folder=t h:BuildFolderTree()end function rQ.SetLibrary(h,t)h.Library=t end function rQ.BuildFolderTree(t)local h={}local N=t.Folder:split("\047")for t=1,#N,1 do h[#h+1]=table.concat(N,"\047",1,t)end table.insert(h,t.Folder)table.insert(h,t.Folder.."\047")for t=1,#h,1 do local N=h[t]if not isfolder(N)then makefolder(N)end end end function rQ.SaveSettings(t)writefile(t.Folder.."\047\111\112\116\105\111\110\115\046\106\115\111\110",HttpService:JSONEncode(rQ.Settings))end function rQ.LoadSettings(t)local h=t.Folder.."\047\11