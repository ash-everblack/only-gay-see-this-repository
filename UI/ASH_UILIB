cal r=W.Folder..("\047\115\101\116\116\105\110\103\115\047"..P)local y=P:match("\040\046\043\041\037\046\106\115\111\110\036")or P local i={configName=y;objects={}}for F,P in pairs(W.Options)do if W.Parser[P.Type]and not W.Ignore[F]then table.insert(i.objects,W.Parser[P.Type].Save(F,P))end end local l,L=pcall(function()return HttpService:JSONEncode(i)end)if not l then return false,"\070\097\105\108\101\100 \116\111 \101\110\099\111\100\101 \100\097\116\097"end writefile(r,L)return true end function Pw.Load(W,F)if not LocalPlayer then return false,"\076\111\099\097\108\080\108\097\121\101\114 \110\111\116 \102\111\117\110\100"end if not W.Library then return false,"\076\105\098\114\097\114\121 \110\111\116 \115\101\116"end local P=F or"\083\101\116\116\105\110\103\115\095"..(LocalPlayer.Name.."\046\106\115\111\110")local r=W.Folder..("\047\115\101\116\116\105\110\103\115\047"..P)if not isfile(r)then return false,"\073\110\118\097\108\105\100 \102\105\108\101"end local y=readfile(r)local i,l=pcall(function()return HttpService:JSONDecode(y)end)if not i then delfile(r)return false,"\068\101\099\111\100\101 \101\114\114\111\114 \040\102\105\108\101 \100\101\108\101\116\101\100\041"end local L=tick()W.Library:Notify({Title=(getgenv()).AshTitle or"\067\111\110\102\105\103 \076\111\097\100\101\114",Content="\076\111\097\100\105\110\103 \099\111\110\102\105\103\046\046\046",SubContent="\080\108\101\097\115\101 \119\097\105\116 \119\104\105\108\101 \115\101\116\116\105\110\103\115 \097\114\101 \098\101\105\110\103 \097\112\112\108\105\101\100\046";Duration=5})local g,k={},{}for F,P in ipairs(l.objects)do local r=W.Parser[P.type]if r and W.Options[P.idx]then local F=function()r.Load(P.idx,P)end if P.type=="\084\111\103\103\108\101"then table.insert(k,F)else table.insert(g,F)end end end task.spawn(function()for F,W in ipairs(g)do W()task.wait(.01)end for F,W in ipairs(k)do W()task.wait(.01)end local F=math.abs(L-tick())local P=string.format("\037\046\050\102",F)local r if F>10 then r="\083\117\099\099\101\115\115\102\117\108\108\121 \108\111\097\100\101\100 \105\110 "..(P.."\115\046 \084\104\097\110\107 \121\111\117 \102\111\114 \121\111\117\114 \112\097\116\105\101\110\099\101 \240\159\165\176")elseif F>5 then r="\076\097\115\116 \067\111\110\102\105\103\117\114\097\116\105\111\110 \104\097\115 \098\101\101\110 \108\111\097\100\101\100 \113\117\105\099\107\108\121 \119\105\116\104\105\110 "..(P.."\115\033")else r="\067\111\110\102\105\103\117\114\097\116\105\111\110 \104\097\115 \098\101\101\110 \108\111\097\100\101\100 \119\105\116\104\105\110 "..(P.."\115")end W.Library:Notify({Title=(getgenv()).AshTitle or"\067\111\110\102\105\103 \076\111\097\100\101\114";Content="\067\111\110\102\105\103 \076\111\097\100\101\100\033",SubContent=r;Duration=6})end)return true end function Pw.IgnoreThemeSettings(F)F:SetIgnoreIndexes({"\073\110\116\101\114\102\097\099\101\084\104\101\109\101","\065\099\114\121\108\105\099\084\111\103\103\108\101";"\084\114\097\110\115\112\097\114\101\110\116\084\111\103\103\108\101";"\077\101\110\117\075\101\121\098\105\110\100"})end function Pw.BuildFolderTree(F)if not isfolder(F.Folder)then makefolder(F.Folder)end local W=F.Folder.."\047\115\101\116\116\105\110\103\115"if not isfolder(W)then makefolder(W)end end function Pw.RefreshConfigList(F)local W={}for F,P in ipairs(listfiles(F.Folder.."\047\115\101\116\116\105\110\103\115"))do if P:sub(-5)=="\046\106\115\111\110"then local F=P:match("\040\091\094\047\092\093\043\041\037\046\106\115\111\110\036")if F and F~="\111\112\116\105\111\110\115"then table.insert(W,F)end end end return W end function Pw.GetAutoloadConfig(F)local W=F.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(W)then return readfile(W)end return nil end function Pw.ClearAutoload(F)local W=F.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(W)then delfile(W)end end function Pw.DeleteConfig(W,F)if not F or F==""then return false,"\073\110\118\097\108\105\100 \099\111\110\102\105\103 \110\097\109\101"end if not F:match("\037\046\106\115\111\110\036")then F=F.."\046\106\115\111\110"end local P=W.Folder..("\047\115\101\116\116\105\110\103\115\047"..F)if not isfile(P)then return false,"\067\111\110\102\105\103 \102\105\108\101 \110\111\116 \102\111\117\110\100"end local r=W.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(r)and readfile(r)==F then delfile(r)end delfile(P)return true end function Pw.LoadAutoloadConfig(F)local W=F.Folder.."\047\115\101\116\116\105\110\103\115\047\097\117\116\111\108\111\097\100\046\116\120\116"if isfile(W)then local P=readfile(W)local r,y=F:Load(P)F.Library:Notify({Title="\073\110\116\101\114\102\097\099\101";Content="\067\111\110\102\105\103 \108\111\097\100\101\114";SubContent=r and"\065\117\116\111 \108\111\097\100\101\100 \099\111\110\102\105\103 "..P or"\070\097\105\108\101\100 \116\111 \108\111\097\100 \097\117\116\111\108\111\097\100 \099\111\110\102\105\103\058 "..y,Duration=7})end end function Pw.BuildConfigSection(W,F)assert(W.Library,"\077\117\115\116 \115\101\116 \083\097\118\101\077\097\110\097\103\101\114\046\076\105\098\114\097\114\121")local P=F:AddSection("\067\111\110\102\105\103\117\114\097\116\105\111\110")P:AddInput("\083\097\118\101\077\097\110\097\103\101\114\095\067\111\110\102\105\103\078\097\109\101",{Title="\067\111\110\102\105\103 \110\097\109\101"})P:AddDropdown("\083\097\118\101\077\097\110\097\103\101\114\095\067\111\110\102\105\103\076\105\115\116",{Title="\067\111\110\102\105\103 \108\105\115\116",Values=W:RefreshConfigList();AllowNull=true})local function r(F,P)W.Library:Notify({Title="\073\110\116\101\114\102\097\099\101",Content="\067\111\110\102\105\103 \108\111\097\100\101\114",SubContent=P;Duration=7})end P:AddButton({Title="\067\114\101\097\116\101 \099\111\110\102\105\103",Callback=function()local F=W.Options.SaveManager_ConfigName.Value if F:match("\094\037\115\042\036")then return r("\073\110\116\101\114\102\097\099\101","\073\110\118\097\108\105\100 \099\111\110\102\105\103 \110\097\109\101 \040\101\109\112\116\121\041")end local P,y=W:Save(F.."\046\106\115\111\110")r("\073\110\116\101\114\102\097\099\101",P and"\067\114\101\097\116\101\100 \099\111\110\102\105\103 "..F or"\070\097\105\108\101\100 \116\111 \115\097\118\101 \099\111\110\102\105\103\058 "..y)W.Options.SaveManager_ConfigList:SetValues(W:RefreshConfigList())W.Options.SaveManager_ConfigList:SetValue(F)end})P:AddButton({Title="\076\111\097\100 \099\111\110\102\105\103",Callback=function()local F=W.Options.SaveManager_ConfigList.Value if not F or F==""then return r("\073\110\116\101\114\102\097\099\101","\078\111 \099\111\110\102\105\103 \115\101\108\101\099\116\101\100")end if not F:match("\037\046\106\115\111\110\036")then F=F.."\046\106\115\111\110"end local P,y=W:Load(F)r("\073\110\116\101\114\102\097\099\101",P and"\076\111\097\100\101\100 \099\111\110\102\105\103 "..F or"\070\097\105\108\101\100 \116\111 \108\111\097\100 \099\111\110\102\105\103\058 "..y)end})P:AddButton({Title="\079\118\101\114\119\114\105\116\101 \099\111\110\102\105\103";Callback=function()local F=W.Options.SaveManager_ConfigList.Value if not F or F==""then return r("\073\110\116\101\114\102\097\099\101","\078\111 \099\111\110\102\105\103 \115\101\108\101\099\116\101\100")end if not F:match("\037\046\106\115\111\110\036")then F=F.."\046\106\115\111\110"end local P,y=W:Save(F)r("\073\110\116\101\114\102\097\099\101",P and"\079\118\101\114\119\114\111\116\101 \099\111\110\102\105\103 "..F or"\070\097\105\108\101\100 \116\111 \111\118\101\114\119\114\105\116\101 \099\111\110\102\105\103\058 "..y)end})P:AddButton({Title="\068\101\108\101\116\101 \099\111\110\102\105\103";Callback=function()local F=W.Options.SaveManager_ConfigList.Value if not F or F==""then return r("\073\110\116\101\114\102\097\099\101","\078\111 \099\111\110\102\105\103 \115\101\10